                <TextBox Height="23" HorizontalAlignment="Left" Margin="48,31,0,0" Name="cpfT" VerticalAlignment="Top" Width="102" IsEnabled="False" />
                <RadioButton Content="F" Height="16" HorizontalAlignment="Left" Margin="567,35,0,0" Name="sexoFT" VerticalAlignment="Top" IsEnabled="False" />
                <Label Content="RG:" Height="28" HorizontalAlignment="Left" Margin="328,28,0,0" Name="label7" VerticalAlignment="Top" />
                <TextBox Height="23" HorizontalAlignment="Left" Margin="362,32,0,0" Name="rgT" VerticalAlignment="Top" Width="106" IsEnabled="False" />
                <Label Content="Dt. Nasc:" Height="28" HorizontalAlignment="Left" Margin="151,30,0,0" Name="label12" VerticalAlignment="Top" />
                <TextBox Height="23" HorizontalAlignment="Left" Margin="207,32,0,0" Name="dataNascimentoT" VerticalAlignment="Top" Width="116" IsEnabled="False" />
                <Label Content="Sexo:" Height="28" HorizontalAlignment="Left" Margin="486,28,0,0" Name="label14" VerticalAlignment="Top" />
                <Label Content="Nome do Pai" HorizontalAlignment="Left" Margin="3,51,0,21" Name="label23" />
