                <TextBox Height="23" HorizontalAlignment="Right" Margin="0,29,9,0" Name="bairroT" VerticalAlignment="Top" Width="224" IsEnabled="False" />
                <Label Content="Número:" Height="28" HorizontalAlignment="Left" Margin="288,2,0,0" Name="label18" VerticalAlignment="Top" />
                <TextBox Height="23" HorizontalAlignment="Right" Margin="0,4,141,0" Name="numeroT" VerticalAlignment="Top" Width="116" IsEnabled="False" />
                <Label Content="UF:" Height="28" HorizontalAlignment="Left" Margin="1,52,0,0" Name="label20" VerticalAlignment="Top" />
                <ComboBox Height="23" HorizontalAlignment="Right" Margin="0,54,438,0" Name="ufT" VerticalAlignment="Top" Width="120" IsEnabled="False">
