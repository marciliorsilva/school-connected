        </GroupBox>
        <GroupBox Header="Interagindo com o Sistema" Height="81" HorizontalAlignment="Left" Margin="298,359,0,0" Name="groupBox3" VerticalAlignment="Top" Width="200">
            <Grid>
                <Label Content="Usuário:" Height="28" HorizontalAlignment="Left" Name="label10" VerticalAlignment="Top" Margin="0,1,0,0" />
                <TextBox Height="23" HorizontalAlignment="Left" Margin="53,3,0,0" Name="usuarioT" VerticalAlignment="Top" Width="122" IsEnabled="False" />
                <Label Content="Senha:" Height="28" HorizontalAlignment="Left" Margin="1,26,0,0" Name="label11" VerticalAlignment="Top" />
                <TextBox Height="23" HorizontalAlignment="Left" Margin="53,29,0,0" Name="senhaT" VerticalAlignment="Top" Width="122" IsEnabled="False" />
            </Grid>
        </GroupBox>
        <GroupBox Header="Contato" Height="115" HorizontalAlignment="Left" Margin="21,357,0,0" Name="groupBox4" VerticalAlignment="Top" Width="270">
            <Grid>
                <Label Content="Fone:" Height="28" HorizontalAlignment="Left" Margin="-1,2,0,0" Name="label8" VerticalAlignment="Top" />
                <TextBox Height="23" HorizontalAlignment="Left" Margin="43,3,0,0" Name="telefoneT" VerticalAlignment="Top" Width="123" IsEnabled="False" />
                <Label Content="Celular:" Height="28" HorizontalAlignment="Left" Margin="-2,25,0,0" Name="label9" VerticalAlignment="Top" />
                <TextBox Height="23" HorizontalAlignment="Left" Margin="43,28,0,0" Name="celularT" VerticalAlignment="Top" Width="123" IsEnabled="False" />
                <Label Content="Email:" Height="28" HorizontalAlignment="Left" Margin="-2,51,0,0" Name="label6" VerticalAlignment="Top" />
                <TextBox Height="23" HorizontalAlignment="Left" Margin="43,53,0,0" Name="emailT" VerticalAlignment="Top" Width="207" IsEnabled="False" />
            </Grid>
        </GroupBox>
        <Rectangle Height="13" HorizontalAlignment="Left" Margin="0,115,0,0" Name="rectangle1" Stroke="{x:Null}" VerticalAlignment="Top" Width="649">
            <Rectangle.Fill>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#00000000" Offset="0" />
                    <GradientStop Color="Transparent" Offset="0.697" />
                    <GradientStop Color="#FF7F7F7F" Offset="0.574" />
                    <GradientStop Color="#003F3F3F" Offset="0.41" />
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
        <Border BorderBrush="Black" BorderThickness="1" Height="56" HorizontalAlignment="Left" Name="border1" VerticalAlignment="Top" Width="663" Background="#FFCFEBF6">
            <Label Content="Funcionário" Height="36" Name="label2" FontSize="24" HorizontalAlignment="Center" FontFamily="Papyrus" Foreground="#FF0F0E0E" />
        </Border>
        <Grid.Background>
            <ImageBrush ImageSource="/ProjetoEscola;component/Images/1.png" />
        </Grid.Background>
