        <ComboBox Height="23" HorizontalAlignment="Left" Margin="499,148,0,0" Name="cargoT" VerticalAlignment="Top" Width="120" IsEnabled="False">
            <ComboBoxItem Content="Coordenador" />
            <ComboBoxItem Content="Professor" />
        </ComboBox>
        <Button Content="Cancelar" Height="23" HorizontalAlignment="Right" Margin="0,482,21,0" Name="cancela" VerticalAlignment="Top" Width="75" Click="cancela_Click" />
        <GroupBox Header="Endereço" Height="106" HorizontalAlignment="Left" Margin="19,253,0,0" Name="groupBox2" VerticalAlignment="Top" Width="615">
            <Grid>
                <Label Content="Rua:" Height="28" HorizontalAlignment="Left" Name="label16" VerticalAlignment="Top" />
                <TextBox Height="23" HorizontalAlignment="Left" Margin="45,4,0,0" Name="ruaT" VerticalAlignment="Top" Width="234" IsEnabled="False" />
                <Label Content="Cidade:" Height="28" HorizontalAlignment="Left" Margin="-2,26,0,0" Name="label22" VerticalAlignment="Top" />
