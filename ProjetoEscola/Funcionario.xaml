                <TextBox Height="23" HorizontalAlignment="Left" Margin="84,56,0,0" Name="nomePaiT" VerticalAlignment="Top" Width="384" IsEnabled="False" />
                <TextBox Height="23" HorizontalAlignment="Left" Margin="84,80,0,0" Name="nomeMaeT" VerticalAlignment="Top" Width="384" IsEnabled="False" />
                <Label Content="Est. Civil:" Height="28" HorizontalAlignment="Left" Margin="468,54,0,0" Name="label13" VerticalAlignment="Top" />
                <TextBox Height="23" HorizontalAlignment="Right" Margin="0,56,6,0" Name="estadoCivilT" VerticalAlignment="Top" Width="75" IsEnabled="False" />
            </Grid>
        </GroupBox>
        <Button Content="Novo" Height="23" HorizontalAlignment="Left" Margin="27,482,0,0" Name="novo" VerticalAlignment="Top" Width="75" Click="novo_Click" />
        <Button Content="Editar" Height="23" HorizontalAlignment="Left" Margin="118,482,0,0" Name="editar" VerticalAlignment="Top" Width="75" Click="editar_Click" />
        <Button Content="Salvar" Height="23" HorizontalAlignment="Left" Margin="472,482,0,0" Name="salvar" VerticalAlignment="Top" Width="75" Click="salvar_Click" />
        <Button Content="Excluir" Height="23" HorizontalAlignment="Left" Margin="208,482,0,0" Name="excluir" VerticalAlignment="Top" Width="75" Click="excluir_Click" />
        <Label Content="CPF:" Height="28" HorizontalAlignment="Left" Margin="202,80,0,0" Name="label1" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="238,81,0,0" Name="cpfConsultaT" VerticalAlignment="Top" Width="169" />
        <Button Content="Consultar" Height="23" HorizontalAlignment="Left" Margin="412,80,0,0" Name="consultar" VerticalAlignment="Top" Width="67" Click="consultar_Click" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
        <Label Content="Nome:" Height="28" HorizontalAlignment="Left" Margin="28,147,0,0" Name="label4" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Right" Margin="0,149,204,0" Name="nomeT" VerticalAlignment="Top" Width="372" IsEnabled="False" />
        <RadioButton Content="M" Height="16" HorizontalAlignment="Left" Margin="556,179,0,0" Name="sexoMT" VerticalAlignment="Top"  IsEnabled="False" />
